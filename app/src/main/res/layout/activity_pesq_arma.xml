<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo_camuflado"
    android:orientation="vertical"
    tools:context=".view.PesqArmaActivity"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text="@string/titulo_pesq_arma"
        android:layout_gravity="center"
        android:background="#e2dfdf"
        android:textColor="#151414"
        android:textStyle="bold"
        android:layout_marginTop="10sp"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_voltar"
        android:textSize="20sp"
        android:textColor="#000"
        android:textStyle="bold"
        android:background="#e6d2d2d2"
        android:layout_margin="5sp"
        android:onClick="voltar"
        />

    <ListView
        android:id="@+id/lvArmamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000"
        android:textStyle="bold"

        >


    </ListView>




</LinearLayout>